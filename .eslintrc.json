{
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended"],
  "rules": {
    "prefer-const": "error",
    "no-var": "error",
    "no-unused-vars": ["error", { "args": "none", "varsIgnorePattern": "^_" }]
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "coverage/",
    ".storybook/"
  ]
}
